CREATE TABLE UserAccount(
   username varchar (30),
   password varchar (30),
   firstName varchar (30),
   lastName varchar (30),
   civic int,
   email varchar (30),
   phone varchar (15),
   street varchar (30),
   postal varchar (10),
   city varchar (30),
   cardNumber varchar (15),
   cardType varchar (15),
   cardHolder varchar (30),
   validYear int,
   validMonth int,
   cvv int,
   PRIMARY KEY (username)
);
