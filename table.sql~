CREATE TABLE UserAccount(
   username varchar (30),
   password varchar (30),
   firstName varchar (30),
   lastName varchar (30),
   civic integer (10),
   email varchar (30),
   phone varchar (15),
   street varchar (30),
   postal varchar (10),
   city varchar (30),
   cardNumber varchar (15),
   cardType varchar (15),
   cardHolder varchar (30),
   validYear integer (2),
   validMonth integer (2),
   cvv integer (3),
   PRIMARY KEY (username)
);

